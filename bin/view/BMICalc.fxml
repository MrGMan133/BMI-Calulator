<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BmiController">
   <children>
      <GridPane layoutX="14.0" layoutY="14.0" prefHeight="271.0" prefWidth="272.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Height" />
            <Label text="Weight" GridPane.rowIndex="1" />
            <Label id="resultLabel" fx:id="resultLabelText" text=" " GridPane.rowIndex="2" />
            <Button id="calculateButton" mnemonicParsing="false" onAction="#calculateButtonClicked" text="Calculate" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField id="heightTextField" fx:id="heightTextField" GridPane.columnIndex="1" />
            <TextField id="weightTextField" fx:id="weightTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
